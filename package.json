{
  "name": "dash-button",
  "version": "1.0.0",
  "description": "A small server that reacts to Amazon Dash buttons on your WiFi network",
  "main": "build/DashButton.js",
  "files": [
    "build"
  ],
  "bin": {
    "dash-button": "build/cli.js"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "gulp build",
    "prepublish": "gulp clean && gulp build",
    "test": "jest",
    "watch": "gulp watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "unmockedModulePathPatterns": [
      "/node_modules/babel-runtime/",
      "/node_modules/lodash.*/",
      "/node_modules/pcap/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ide/dash-button.git"
  },
  "keywords": [
    "amazon",
    "dash",
    "button"
  ],
  "author": "James Ide",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ide/dash-button/issues"
  },
  "homepage": "https://github.com/ide/dash-button#readme",
  "devDependencies": {
    "babel-core": "^6.0.18",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-plugin-syntax-async-functions": "^6.0.14",
    "babel-plugin-syntax-flow": "^6.0.14",
    "babel-plugin-syntax-trailing-function-commas": "^6.0.14",
    "babel-plugin-transform-async-to-generator": "^6.0.14",
    "babel-plugin-transform-es2015-arrow-functions": "^6.0.14",
    "babel-plugin-transform-es2015-classes": "^6.0.15",
    "babel-plugin-transform-es2015-destructuring": "^6.0.18",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.0.18",
    "babel-plugin-transform-es2015-parameters": "^6.0.18",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.0.14",
    "babel-plugin-transform-es2015-spread": "^6.0.14",
    "babel-plugin-transform-flow-strip-types": "^6.0.14",
    "babel-plugin-transform-function-bind": "^6.0.14",
    "babel-plugin-transform-runtime": "^6.0.16",
    "babel-plugin-transform-strict-mode": "^6.0.15",
    "eslint": "^1.8.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^6.0.0",
    "gulp-changed": "^1.3.0",
    "jest-cli": "^0.7.0",
    "rimraf": "^2.4.3"
  },
  "dependencies": {
    "babel-runtime": "^6.0.14",
    "lodash.padleft": "^3.1.1",
    "pcap": "mranney/node_pcap#21c5398",
    "yargs": "^3.29.0"
  }
}
