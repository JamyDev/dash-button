{
  "name": "dash-button",
  "version": "0.1.0",
  "description": "A small server that reacts to Amazon Dash buttons on your WiFi network",
  "main": "build/DashButton.js",
  "bin": {
    "dash-button": "build/cli.js"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "gulp build",
    "prepublish": "gulp build",
    "test": "jest",
    "watch": "gulp watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": ["<rootDir>/src"],
    "unmockedModulePathPatterns": [
      "/node_modules/babel-runtime/",
      "/node_modules/lodash.*/",
      "/node_modules/pcap/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ide/dash-button.git"
  },
  "keywords": [
    "amazon",
    "dash",
    "button"
  ],
  "author": "James Ide",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ide/dash-button/issues"
  },
  "homepage": "https://github.com/ide/dash-button#readme",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "eslint": "^1.6.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^5.2.1",
    "gulp-changed": "^1.3.0",
    "jest-cli": "^0.5.10",
    "rimraf": "^2.4.3"
  },
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "lodash.padleft": "^3.1.1",
    "pcap": "mranney/node_pcap",
    "yargs": "^3.27.0"
  }
}
