{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["es2019", "esnext.asynciterable"],
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./build",
    "rootDir": "./src",
    "typeRoots": ["node_modules/@types", "./ts-declarations"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  }
}
